defmodule ${1:app}.${2:model} do
  use $1.Web, :model

  ${3:@primary_key {:id, :binary_id, autogenerate: true}}
  schema "${4:schema}" do
    $0

    timestamps
  end

  @required_fields ~w()
  @optional_fields ~w()

  def changeset(model, params \\\ :empty) do
